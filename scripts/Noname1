SELECT UG.*, PT.*, LC.*, PI.*, UN.*, IVD.DOCUMENT_DATE, IVD.DOCUMENT_NO, IVD.COMMIT_FLAG, IVD.DOCUMENT_TYPE
FROM IMPORT_ITEM UG
LEFT OUTER JOIN PART_ITEM PI ON (UG.PART_ITEM_ID = PI.PART_ITEM_ID)
LEFT OUTER JOIN PART_TYPE PT ON (PI.PART_TYPE = PT.PART_TYPE_ID)
LEFT OUTER JOIN LOCATION LC ON (UG.LOCATION_ID = LC.LOCATION_ID)
LEFT OUTER JOIN UNIT UN ON (PI.UNIT_COUNT = UN.UNIT_ID)
LEFT OUTER JOIN INVENTORY_DOC IVD ON (UG.INVENTORY_DOC_ID = IVD.INVENTORY_DOC_ID)
WHERE (INVENTORY_DOC_ID = 12604) ORDER BY IMPORT_ITEM_ID ASC


SELECT *
FROM IMPORT_ITEM UG
LEFT OUTER JOIN PART_ITEM PI ON (UG.PART_ITEM_ID = PI.PART_ITEM_ID)
LEFT OUTER JOIN PART_TYPE PT ON (PI.PART_TYPE = PT.PART_TYPE_ID)
LEFT OUTER JOIN LOCATION LC ON (UG.LOCATION_ID = LC.LOCATION_ID)
LEFT OUTER JOIN UNIT UN ON (PI.UNIT_COUNT = UN.UNIT_ID)
WHERE (INVENTORY_DOC_ID = 12604)

LEFT OUTER JOIN INVENTORY_DOC IVD ON (UG.INVENTORY_DOC_ID = IVD.INVENTORY_DOC_ID)

WHERE (INVENTORY_DOC_ID = 12604)
ORDER BY IMPORT_ITEM_ID ASC


CREATE TABLE "INVENTORY_DOC_EXT" EXTERNAL 'C:\INVENTORY_DOC.DAT'
(
  "INVENTORY_DOC_ID"	"ID_TYPE" NOT NULL,
  "DOCUMENT_NO"	"CODE_TYPE" NOT NULL,
  "DOCUMENT_DATE"	"DATE_TYPE" NOT NULL,
  "DOCUMENT_DESC"	"DESC_TYPE",
  "BILL_NO"	"CODE_TYPE",
  "DO_NO"	"CODE_TYPE",
  "TRUCK_NO"	"CODE_TYPE",
  "SUPPLIER_ID"	"ID_TYPE",
  "DELIVERY_ID"	"ID_TYPE",
  "DELIVERY_FEE"	"MONEY_TYPE",
  "SENDER_NAME"	"NAME_TYPE",
  "RECEIVE_NAME"	"NAME_TYPE",
  "DOCUMENT_TYPE"	"ID_TYPE" NOT NULL,
  "CREATE_DATE"	"DATE_TYPE" NOT NULL,
  "CREATE_BY"	"ID_TYPE" NOT NULL,
  "MODIFY_DATE"	"DATE_TYPE" NOT NULL,
  "MODIFY_BY"	"ID_TYPE" NOT NULL,
  "EMP_ID"	"ID_TYPE",
  "COMMIT_FLAG"	"FLAG_TYPE",
  "SALE_FLAG"	"FLAG_TYPE",
  "EXCEPTION_FLAG"	"FLAG_TYPE",
CONSTRAINT "INVENTORY_DOC_ID_PK" PRIMARY KEY ("INVENTORY_DOC_ID")
);
INSERT INTO INVENTORY_DOC_EXT SELECT * FROM INVENTORY_DOC WHERE (CREATE_DATE <= '2005-05-20 23:59:59') AND (CREATE_DATE >= '2005-05-20 00:00:00');

INSERT INTO INVENTORY_DOC SELECT * FROM INVENTORY_DOC_EXT WHERE (CREATE_DATE <= '2005-05-20 23:59:59') AND (CREATE_DATE >= '2005-05-20 00:00:00');


