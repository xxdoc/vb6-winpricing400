Public Function ShowDescText() As String
   If MANUAL_FLAG = "N" Then
      If DISPLAY_ID = 1 Then
         ShowDescText = ITEM_DESC
      ElseIf DISPLAY_ID = 2 Then
         ShowDescText = FEATURE_DESC
      ElseIf DISPLAY_ID = 3 Then
         ShowDescText = PART_DESC
